#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2003/11/swrl#equal
<http://www.w3.org/2003/11/swrl#equal> rdf:type rdfs:Datatype .


###  http://www.w3.org/2003/11/swrl#lessThan
<http://www.w3.org/2003/11/swrl#lessThan> rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/oventura/TheMoneyHeist#Has_Cast
:Has_Cast rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          rdfs:domain :Cast ;
          rdfs:range :Episodic ,
                     :Main ,
                     :Secondary ,
                     :Tertiary ;
          rdfs:comment "Corresponde al nivel de participación del personaje en la serie." .


###  http://www.semanticweb.org/oventura/TheMoneyHeist#Has_Character
:Has_Character rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty ;
               rdfs:domain :Character ;
               rdfs:range :Bank_Workers ,
                          :Customers ,
                          :GEOs ,
                          :Hostage ,
                          :Military ,
                          :Mint_Workers ,
                          :Negotiator ,
                          :Others ,
                          :Police ,
                          :Police_Agent ,
                          :Police_Chief ,
                          :Robber ,
                          :Security_Forces ;
               rdfs:comment "Corresponde al rol del personaje en la serie. (Atracador, policia, extras, etc.)" .


###  http://www.semanticweb.org/oventura/TheMoneyHeist#Has_Gender
:Has_Gender rdf:type owl:ObjectProperty ,
                     owl:FunctionalProperty ;
            rdfs:domain :Person ;
            rdfs:range :Actor ,
                       :Actress ;
            rdfs:comment "Nos indica el genero del personaje, masculino o femenino." .


###  http://www.semanticweb.org/oventura/TheMoneyHeist#Has_ProminentAttribute
:Has_ProminentAttribute rdf:type owl:ObjectProperty ,
                                 owl:TransitiveProperty ;
                        rdfs:domain :Character ;
                        rdfs:range :Robber ;
                        rdfs:comment "Nos indica la cualidad más destacable del miembro de la banda." .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/oventura/TheMoneyHeist#Has_Dead
:Has_Dead rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Robber ,
                      :Security_Forces ;
          rdfs:range xsd:boolean ;
          rdfs:comment "Nos indica si un miembro de la banda muere durante la serie." .


###  http://www.semanticweb.org/oventura/TheMoneyHeist#Has_NickName
:Has_NickName rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :Robber ;
              rdfs:range xsd:string ;
              rdfs:comment "Nos indica el apodo e algunos de los miembros de la banda." .


###  http://www.semanticweb.org/oventura/TheMoneyHeist#Has_NumberChapter
:Has_NumberChapter rdf:type owl:DatatypeProperty ;
                   rdfs:subPropertyOf owl:topDataProperty ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain :Character ;
                   rdfs:range xsd:integer ;
                   rdfs:comment "Cooresponde a los número de los capítulos" .


###  http://www.semanticweb.org/oventura/TheMoneyHeist#Has_Role
:Has_Role rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain :Bank_Workers ,
                      :Customers ,
                      :Hostage ,
                      :Military ,
                      :Mint_Workers ,
                      :Others ,
                      :Police ,
                      :Robber ,
                      :Security_Forces ;
          rdfs:range xsd:string ;
          rdfs:comment "Nos indica el rol de los actores de la serie." .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/oventura/TheMoneyHeist#Actor
:Actor rdf:type owl:Class ;
       rdfs:subClassOf :Person ;
       owl:disjointWith :Actress ;
       rdfs:comment "Los actores masculinos de la serie." .


###  http://www.semanticweb.org/oventura/TheMoneyHeist#Actress
:Actress rdf:type owl:Class ;
         rdfs:subClassOf :Person ;
         rdfs:comment "Las actrices femeninas de la serie." .


###  http://www.semanticweb.org/oventura/TheMoneyHeist#Bank_Workers
:Bank_Workers rdf:type owl:Class ;
              rdfs:subClassOf :Hostage ;
              owl:disjointWith :Customers ;
              rdfs:comment "Trabajadores del Banco de España" .


###  http://www.semanticweb.org/oventura/TheMoneyHeist#Bank_of_Spain
:Bank_of_Spain rdf:type owl:Class ;
               rdfs:subClassOf :Location ;
               rdfs:comment "Es el Banco de España, donde se desarrollan dos temporadas de la serie." ;
               rdfs:sameAs "https://es.dbpedia.org/page/Banco_de_Espa%C3%B1a" .


###  http://www.semanticweb.org/oventura/TheMoneyHeist#Cast
:Cast rdf:type owl:Class ;
      rdfs:subClassOf :Serie ;
      rdfs:comment "El reparto de los actores según su participación en la serie." .


###  http://www.semanticweb.org/oventura/TheMoneyHeist#Character
:Character rdf:type owl:Class ;
           rdfs:comment "Los diferentes roles de los actores que han participado en la serie." .


